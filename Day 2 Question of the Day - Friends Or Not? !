#include <iostream>
#include <map>
using namespace std;

int main() {
    int n;
    cin >> n;

    map<int, int> friends;
    for (int i = 1; i <= n; i++) {
        friends[i] = 0;
    }

    int a, b;
    for (int i = 0; i < n - 1; i++) {
        cin >> a >> b;
        friends[a]++;
        friends[b]++;
    }

    for (auto f : friends) {
        if (f.second == n - 1) {
            cout << "Yes" << endl;
            return 0;
        }
    }

    cout << "No" << endl;
    return 0;
}
